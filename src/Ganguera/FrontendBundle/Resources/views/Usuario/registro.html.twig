{% extends '::frontend.html.twig' %}

{% block title %}{{ title }}{% endblock %}

{% block contenido %}
<div class="row-fluid">
    <form class="form-horizontal" action="" method="post" {{ form_enctype(form) }}>
        <fieldset>
            <legend>Reg&iacute;strese</legend>
            {#{ form_widget(form) }#}
            {#{ form_row(form.nombre) }#}
            
            {% include 'FrontendBundle:Usuario:campos_registro_usuario.html.twig' with {'formulario': form} only %}
        </fieldset>
        {{ form_rest(form) }}
    </form>
</div>
{% endblock %}